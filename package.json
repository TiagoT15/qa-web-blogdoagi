{
  "name": "qa-web-blogdoagi-cypress",
  "version": "1.0.0",
  "description": "Automação E2E para busca no Blog do Agi - Cypress + TypeScript",
  "main": "index.js",
  "scripts": {
    "test:headless": "cypress run",
    "test:headed": "cypress open",
    "report:merge": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/report.json",
    "report:html": "marge cypress/reports/mochawesome/report.json --reportDir cypress/reports/html",
    "test:full": "npm run test:headless && npm run report:merge && npm run report:html"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/node": "^20.10.0",
    "chai": "^4.3.10",
    "cypress": "^13.6.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "cypress",
    "typescript",
    "qa",
    "automation",
    "e2e",
    "blog",
    "agibank"
  ],
  "author": "QA Engineer",
  "license": "MIT"
}
